/*Use a SELECT statment to order the items by price.*/
SELECT item_name, price
FROM superstore
ORDER BY price;

/*Show a statistic about the stock quantity.*/
SELECT SUM(stock_quantity)
FROM superstore;

/*Show a statistic about the price for items in the category of "Kitchen Supplies".*/
SELECT AVG(price), "Kitchen_Supplies"
FROM superstore;

/*Show the stock quantity of "Air Purifiers" and change column name.*/
SELECT SUM(stock_quantity) AS total_air_purifiers_in_stock
FROM superstore
WHERE item_name = "Air Purifier";
