CREATE TABLE IF NOT EXISTS Walmart (
    invoice_id INT,
    branch VARCHAR(10),
    city VARCHAR(20),
    customer_type VARCHAR(10),
    gender VARCHAR(10),
    product_line VARCHAR(50),
    unit_price FLOAT,
    quantity INT,
    vat FLOAT,
    total FLOAT,
    date DATE,
    time TIME,
    payment VARCHAR(20),
    cogs FLOAT,
    gross_margin_percentage FLOAT,
    gross_income FLOAT,
    rating FLOAT
);
